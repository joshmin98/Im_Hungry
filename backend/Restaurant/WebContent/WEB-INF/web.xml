<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 

xmlns="http://xmlns.jcp.org/xml/ns/javaee" 

xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 

id="WebApp_ID" version="3.1">
  <display-name>Im Hungry - Yelp Restaurant</display-name>
  <welcome-file-list>
    <!-- <welcome-file>index.html</welcome-file> -->
    <welcome-file>index.html</welcome-file>
   
  </welcome-file-list>
  <servlet>  
     <!-- class name -->  
    <servlet-name>RestaurantServlet</servlet-name>  
    <!-- package-->  
    <servlet-class>com.YelpRestaurant.Servlets.RestaurantServlet</servlet-class>  
  </servlet>  
  <servlet-mapping>  
    <servlet-name>RestaurantServlet</servlet-name>  
    <!-- address -->  
    <url-pattern>/RestaurantServlet/RestaurantServlet</url-pattern>  
    </servlet-mapping> 
</web-app>